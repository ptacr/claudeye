{
  "name": "claudeye",
  "version": "0.3.3",
  "description": "Watchtower for Claude Code & Agents SDK â€” replay sessions, run custom evals, debug agent traces locally",
  "bin": {
    "claudeye": "./bin/claudeye.mjs"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "bin/",
    "dist/",
    ".next/standalone/",
    "README.md"
  ],
  "engines": {
    "node": ">=20.9.0"
  },
  "scripts": {
    "dev": "tsx scripts/dev.ts --port 8020",
    "build": "next build",
    "build:evals": "tsc -p tsconfig.evals.json",
    "start": "tsx scripts/start.ts",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint . --config eslint.config.mjs",
    "prepublishOnly": "npm run build:evals && npm run build && node scripts/prepare-package.mjs"
  },
  "keywords": [
    "claude",
    "claude-code",
    "claude-agents-sdk",
    "anthropic",
    "ai-agent",
    "llm-observability",
    "agent-observability",
    "log-viewer",
    "session-replay",
    "eval-framework",
    "developer-tools",
    "devtools",
    "cli",
    "dashboard",
    "local-first",
    "monitoring",
    "debugging",
    "tracing"
  ],
  "author": "ExosphereHost Inc. <claudeye@exosphere.host>",
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "https://github.com/exospherehost/claudeye.git"
  },
  "homepage": "https://github.com/exospherehost/claudeye#readme",
  "bugs": {
    "url": "https://github.com/exospherehost/claudeye/issues"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@tanstack/react-virtual": "^3.13.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^25.2.0",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.24",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.6",
    "jsdom": "^24.1.3",
    "lucide-react": "^0.563.0",
    "next": "^16.1.6",
    "postcss": "^8.5.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "vite": "^6.4.1",
    "vitest": "^3.2.4"
  }
